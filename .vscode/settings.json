{
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.associations": {
        "*.js": "javascriptreact"
    },
    "javascript.format.enable": false,
    "typescript.format.enable": false,
    "prettier.requireConfig": true,
    "prettier.trailingComma": "all",
    "prettier.singleQuote": true,
    "prettier.printWidth": 80,
    "prettier.tabWidth": 2,
    "prettier.useTabs": false,
    "prettier.semi": true,
    "prettier.arrowParens": "always",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.alwaysShowStatus": true,
    "chat.showWelcomeMessage": false,
    "chat.notifyWindowOnConfirmation": "off",
    "circleci.filters.branchFilter": "allBranches",
    "circleci.filters.statusFilter": {
        "canceled": true,
        "failed": true,
        "failing": true,
        "not_run": true,
        "on_hold": true,
        "success": true
    },
    "circleci.notifications.statusFilter": {
        "canceled": true,
        "error": true,
        "failed": true,
        "failing": true,
        "not_run": true,
        "on_hold": true,
        "success": true,
        "unauthorized": true
    },
    "circleci.persistedProjectSelection": [
        "gh/AtonixCorp/atonixcorp"
    ]
}
